condicionesLluvia (){
    lluvia1 = chispear
    lluvia2 = fuerte
    clima = lluvia1
    
    salirSinParaguas = si
    salir = no
    
    if(clima = lluvia1) {
        salirSinParaguas = si
        irAlaTienda()
    }   
    else (clima = lluvia2){
        salir = no
        quedarseEnCasa()
    }
}